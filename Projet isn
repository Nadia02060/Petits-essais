<script type="text/javascript">

function main() {
   var xmin = -10;
   var xmax = 10;
   var xgrad = 1;
   var ymin = -10;
   var ymax = 10;
   var ygrad = 1;
   var plot = document.getElementById("curve"); 
   var pen = plot.getContext("2d");
   var x0 = plot.height*(-xmin)/(xmax-xmin)
   var y0 = plot.width* (-ymax)/(ymin-ymax)
   pen.beginPath();
   pen.moveTo(450,10);
   pen.lineTo(plot.height,y0);
   pen.moveTo(-50000,-10);
   pen.lineTo(plot.width,x0);
   pen.stroke();
   xgrad*Math.floor (xmax/xgrad)
   xgrad*Math.Ceil  (xmax/xgrad)
   while  (x<x1)
   xe=
   moveTo ()
   lineTo (0)
   stroke()
   x=x+xgrad
 }  
   


</script>
